               <cell>
<persName xml:id="1191">Scott&#8211;Holland</persName></cell>
               <cell>
<persName xml:id="393">Shakespeare</persName></cell>
            </row>
            <row>
               <cell>
<persName xml:id="2523">Simpson</persName></cell>
               <cell>
<persName xml:id="2608">Adeney</persName>.</cell>
            </row>
            <row>
               <cell>
<persName xml:id="545">Dean of Lincoln</persName></cell>
               <cell>
<persName xml:id="2609">Peake</persName></cell>
